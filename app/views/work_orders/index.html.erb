<div class = "container">

<h1>Listing work_orders</h1>

<table>
  <thead>
    <tr>
      <th>Starttime</th>
      <th>Endtime</th>
      <th>Customerid</th>
      <th>Languageto</th>
      <th>Languagefrom</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_orders.each do |work_order| %>
      <tr>
        <td><%= work_order.startTime %></td>
        <td><%= work_order.endTime %></td>
        <td><%= work_order.customerId %></td>
        <td><%= work_order.languageTo %></td>
        <td><%= work_order.languageFrom %></td>
        <td><%= link_to 'Show', work_order %></td>
        <td><%= link_to 'Edit', edit_work_order_path(work_order) %></td>
        <td><%= link_to 'Destroy', work_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work order', new_work_order_path %>
</div>